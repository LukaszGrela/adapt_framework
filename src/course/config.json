{
    "_defaultLanguage": "en",
    "_defaultDirection": "ltr",
    "_questionWeight": 1,
    "_logging": {
        "_isEnabled": true,
        "_level": "debug",
        "_console": true
    },
    "_accessibility": {
        "_isEnabled": true,
        "_isDisabledOnTouchDevices": false,
        "_shouldSupportLegacyBrowsers": true,
        "_isTextProcessorEnabled": true,
        "_isSkipNavigationEnabled": true
    },
    "_drawer": {
        "_showEasing": "easeOutQuart",
        "_hideEasing": "easeInQuart",
        "_duration": 400
    },
    "_spoor": {
        "_isEnabled": true,
        "_tracking": {
            "_requireCourseCompleted": true,
            "_requireAssessmentPassed": true,
            "_shouldSubmitScore": false,
            "_shouldStoreResponses": false,
            "_shouldRecordInteractions": true
        },
        "_reporting": {
            "_comment": "Your options here are 'completed', 'passed', 'failed', and 'incomplete'",
            "_onTrackingCriteriaMet": "completed",
            "_onAssessmentFailure": "incomplete",
            "_resetStatusOnLanguageChange": false
        }
    },
    "_activity": {
        "_isEnabled": true,
        "_verbs": [
            "abandoned",
            "answered",
            "asked",
            "attempted",
            "commented",
            "completed",
            "exited",
            "experienced",
            "failed",
            "imported",
            "initialized",
            "interacted",
            "launched",
            "logged_in",
            "logged_out",
            "mastered",
            "passed",
            "preferred",
            "progressed",
            "registered",
            "responded",
            "resumed",
            "satisfied",
            "scored",
            "shared",
            "suspended",
            "terminated",
            "voided",
            "waived"
        ],
        "_activityTypes": [
            "assessment",
            "attempt",
            "block",
            "course",
            "file",
            "interaction",
            "lesson",
            "link",
            "media",
            "meeting",
            "module",
            "objective",
            "performance",
            "profile",
            "question",
            "simulation"
        ]
    },
    "_disableAnimationFor": [".ie8", ".iPhone.version-7\\.0"],
    "build": {
    }
}
